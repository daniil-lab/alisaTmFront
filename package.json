{
  "name": "react-redux-ts-boilerplate",
  "version": "0.0.1",
  "homepage": "https://daniil-lab.github.io/alisatmfront",
  "description": "Boilerplate for typesafe React + Redux + Typescript.",
  "main": "src/index.tsx",
  "author": "ebkn",
  "license": "MIT",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "prestart": "yarn typecheck",
    "start": "concurrently \"yarn webpack:dev\" \"yarn tsserver\"",
    "webpack:dev": "webpack serve",
    "tsserver": "tsc --watch",
    "prebuild": "yarn typecheck",
    "build": "webpack",
    "prebuild:prod": "yarn typecheck",
    "build:prod": "webpack --mode production",
    "prelint": "yarn typecheck",
    "lint": "tslint './src/**/*.ts{,x}'",
    "prelint:fix": "yarn typecheck",
    "lint:fix": "tslint --fix './src/**/*.ts{,x}'",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook",
    "deploy": "gh-pages -d build",
    "prettier:fix": "prettier src/*"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/react": "^5.1.9",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "5.1.2",
    "@types/react-router-dom": "5.1.2",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "css-loader": "^3.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^4.2.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "5.1.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "style-loader": "^2.0.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^4.0.0",
    "typescript": "^4.2.3",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^1.2.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@svgr/webpack": "^5.5.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "node-sass": "^5.0.0",
    "react-svg-loader": "^3.0.3",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "svg-url-loader": "^7.1.1",
    "url-loader": "^4.1.1"
  }
}
